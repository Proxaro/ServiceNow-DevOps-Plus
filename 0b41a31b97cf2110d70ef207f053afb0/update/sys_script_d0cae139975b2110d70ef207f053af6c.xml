        <script><![CDATA[/* global g_scratchpad, current, previous */
/* eslint no-undef: "error" */
(function executeRule(current, previous /*null when async*/) {

	g_scratchpad.devops_plus = {
		update_name: current.sys_update_name.toString(),
		fields: [{
			name: 'sys_scope',
			label: current.sys_scope.getLabel(),
			value: current.getValue('sys_scope'),
			display_value: current.getDisplayValue('sys_scope'),
			type: 'reference',
			edit: false
		}]
	};

	if (current.isValidField('description')) {
		g_scratchpad.devops_plus.fields.push({
			name: 'description',
			label: current.description.getLabel(),
			value: current.getValue('description'),
			display_value: current.getDisplayValue('description'),
			type: 'string',
			edit: true && current.canWrite()
		});
	}

})(current, previous);]]></script>
